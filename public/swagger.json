"/api/tools/check-host": {
  "get": {
    "tags": ["TOOLS"],
    "summary": "Cek status host/domain",
    "description": "Memeriksa status host atau domain menggunakan tipe koneksi tertentu. Tipe yang tersedia: `ping`, `http`, `udp`, `tcp`, `info`.\n\nContoh request: `https://api-yudzxzy.vercel.app/api/tools/check-host?host=example.com&type=http`",
    "servers": [
      { "url": "https://api-yudzxzy.vercel.app", "description": "Server utama" }
    ],
    "parameters": [
      {
        "name": "host",
        "in": "query",
        "required": true,
        "description": "Host atau domain yang ingin diperiksa",
        "schema": {
          "type": "string",
          "example": "https://api-yudzxzy.vercel.app"
        }
      },
      {
        "name": "type",
        "in": "query",
        "required": true,
        "description": "Tipe pengecekan host: ping, http, udp, tcp, info",
        "schema": {
          "type": "string",
          "example": "http",
          "enum": ["ping", "http", "udp", "tcp", "info"]
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Host berhasil diperiksa",
        "content": {
          "application/json": {
            "example": {
              "status": 200,
              "author": "Yudzxml",
              "host": "example.com",
              "type": "http",
              "online": true,
              "responseTime": "123ms",
              "info": "Host aktif dan merespon HTTP"
            }
          }
        }
      },
      "400": { "description": "Parameter `host` atau `type` kurang / tidak valid" },
      "404": { "description": "Host tidak ditemukan" }
    }
  }
},
"/api/search/jadwalshalat": {
  "get": {
    "tags": ["ISLAMIC"],
    "summary": "Jadwal Shalat Harian",
    "description": "Mengambil jadwal shalat harian berdasarkan kota. Parameter `kota` wajib diisi.\n\nContoh request: `https://api-yudzxzy.vercel.app/api/search/jadwalshalat?kota=Karimun`",
    "servers": [
      { "url": "https://api-yudzxzy.vercel.app", "description": "Server utama" }
    ],
    "parameters": [
      {
        "name": "kota",
        "in": "query",
        "required": true,
        "description": "Nama kota untuk jadwal shalat",
        "schema": {
          "type": "string",
          "example": "Karimun"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mengambil jadwal shalat",
