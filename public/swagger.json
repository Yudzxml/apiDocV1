  "/api/ytdl": {
  "get": {
    "summary": "YouTube Video Downloader",
    "description": "Endpoint ini digunakan untuk mengambil video YouTube berdasarkan URL, kualitas, dan format.",
    "servers": [
      {
        "url": "https://api-yudzxml.koyeb.app",
        "description": "Server untuk download ytdl"
      }
    ],
    "parameters": [
      {
        "name": "url",
        "in": "query",
        "required": true,
        "description": "URL video YouTube yang ingin diunduh",
        "schema": {
          "type": "string",
          "example": "https://youtube.com/watch?v=Fp154clV4pk"
        }
      },
      {
        "name": "quality",
        "in": "query",
        "required": false,
        "description": "Resolusi yang diinginkan video: (360, 480, 720, 1080.),  audio: ( 320, 256, 192, 128 ) ",
        "schema": {
          "type": "integer",
          "example": 360
        }
      },
      {
        "name": "format",
        "in": "query",
        "required": false,
        "description": "Format file yang diinginkan, bisa 'video' atau 'audio'",
